<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/ankittiwari"
    android:layout_height="match_parent"
    tools:context="com.example.archit.gomusiclive.Fragment_ankit">

    <!-- TODO: Update blank fragment layout -->
   <RelativeLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent">

       <Button
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="Get Live Shows"
           android:id="@+id/btnGetLiveAn"
           android:onClick="getLiveAnkit"
           android:layout_alignParentTop="true"
           android:layout_alignParentStart="true"
           android:layout_marginTop="87dp" />

       <Button
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="Get Music Videos"
           android:id="@+id/btnGetVideoAn"
           android:onClick="getMusicAnkit"
           android:layout_centerVertical="true"
           android:layout_alignParentStart="true" />

       <Button
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="Get Details"
           android:id="@+id/btnGetDetailAn"
           android:onClick="getWikiAnkit"
           android:layout_below="@+id/btnGetVideoAn"
           android:layout_alignParentStart="true"
           android:layout_marginTop="78dp" />
   </RelativeLayout>
</FrameLayout>
